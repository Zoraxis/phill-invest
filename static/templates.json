{
    "caroseul": {
        "markup": "<div data-num=\"{num}\" style=\"transform: translateX(-19.5vh)\"\n class=\"w-[{w}] h-[{h}] absolute bottom-[{bottom}] left-[17.7vw] rounded-[80px] overflow-hidden flex justify-end flex-col pl-[2.9vh] pb-[5vh] caroseul-item z-[{z}]\">\n                        <img class=\"top-0 left-0 w-full h-full absolute object-cover\" src=\"media/img/{src}\"\n                            alt=\"c\" />\n                        <div class=\"absolute left-0 -bottom-[7vh] w-full gradient-bottom h-full z-[1]\"></div>\n                        <object class=\"top-1/2 -translate-y-1/2 left-1/2 -translate-x-1/2 absolute w-[6.9vh]\"\n                            data=\"media/icons/play.svg\"></object>\n                        <div class=\"text-rows z-[2]\">\n                            <span class=\"text-[#84E644] text-[19px] font-medium capitalize caroseul-title\">{title}</span>\n                            <span class=\"text-white text-[17px] font-normal caroseul-author\">{author}</span>\n                        </div>\n                    </div>"
    },
    "slider": {
        "markup": "<div class=\"h-[680px] w-[487px] p-[16.5px] flex flex-col justify-end rounded-[30px] relative\">\n                    <img class=\"absolute top-0 left-0 w-full h-full\" src=\"media/img/{src}\" alt=\"pro\" />\n                    <div class=\"h-[180px] w-full bg-white z-[2] rounded-[20px] p-6 flex flex-col gap-8\">\n                        <span class=\"text-[#030043] text-xl font-medium\">{text}</span>\n                        <a href=\"#\"\n                            class=\"flex gap-3 rounded-[40px] bg-[#84E644] text-[#001749] px-6 py-[15px] w-max text-[15px] font-semibold\">\n                            <object data=\"media/icons/play-tri.svg\"></object>\n                            <span class=\"uppercase\">{button}</span>\n                        </a>\n                    </div>\n                </div>"
    },
    "header": {
        "markup": "<a href=\"{href}\">{label}</a>"
    }
}