{
    "caroseul": {
        "markup": "<div data-link=\"{href}\" data-num=\"{num}\" style=\"transform: translateX({transform})\"\n class=\"w-[{w}] h-[{h}] absolute bottom-[{bottom}] left-[17.7vw] rounded-[60px] overflow-hidden flex justify-end flex-col pl-[2.9vh] pb-[5vh] caroseul-item z-[{z}]\">\n                        <img class=\"top-0 left-0 w-full h-full absolute object-cover\" src=\"media/img/{src}\"\n                            alt=\"c\" />\n                        <div class=\"pointer-events-none absolute left-0 -bottom-[7vh] w-full gradient-bottom h-full z-[1]\"></div>\n                        <img style=\"transform: translate(-50%, -50%)\" class=\"carousel-item-play btn top-1/2 left-1/2 absolute w-[6.9vh]\"\n                            src=\"media/icons/play.svg\"></img>\n                        <div class=\"text-rows z-[2]\">\n                            <span class=\"text-[#84E644] text-[19px] font-medium capitalize caroseul-title\">{title}</span>\n                            <span class=\"text-white text-[17px] font-normal caroseul-author\">{author}</span>\n                        </div>\n                    </div>"
    },
    "slider": {
        "markup": "<div class=\"max-sm:px-[44px] max-sm:pb-[36px] h-[{h}] w-[{w}] p-[16.5px] flex flex-col justify-end rounded-[30px] relative\">\n                    <img class=\"absolute rounded-[30px] top-0 left-0 w-full h-full\" src=\"media/img/{src}\" alt=\"pro\" />\n                    <div class=\"max-sm:hidden h-[180px] w-full bg-white z-[2] rounded-[20px] p-6 flex flex-col gap-8\">\n                        <span class=\"text-[#030043] text-xl font-medium\">{text}</span>\n                        <a target=\"_blank\" href=\"#\"\n                            class=\"flex gap-3 rounded-[40px] bg-[#84E644] text-[#001749] px-6 py-[15px] w-max text-[15px] font-semibold btn\">\n                            <object data=\"media/icons/play-tri.svg\"></object>\n                            <span class=\"uppercase\">{button}</span>\n                        </a>\n                    </div>\n      <a target=\"_blank\" href=\"#\"\n                            class=\"max-sm:flex z-[1] hidden gap-3 rounded-[40px] bg-[#84E644] text-[#001749] px-6 py-[15px] w-max text-[15px] font-semibold btn\">\n                            <object data=\"media/icons/play-tri.svg\"></object>\n                            <span class=\"uppercase\">{button}</span>\n                        </a>\n          </div>"
    },
    "header": {
        "markup": "<a href=\"{href}\">{label}</a>"
    }
}